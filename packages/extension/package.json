{
  "name": "@yi-note/extension",
  "version": "1.4.1",
  "description": "Most efficient way to take & share time-stamped notes while watching videos!",
  "private": false,
  "keywords": [
    "chrome",
    "extension",
    "elearning",
    "notes"
  ],
  "main": "src/index.js",
  "scripts": {
    "build:chromium": "webpack --env=chromium --config webpack.config.js",
    "build:firefox": "webpack --env=firefox --config webpack.config.js",
    "build": "npm run build:chromium && npm run build:firefox",
    "dev:chromium": "webpack --env=chromium --config webpack.dev.config.js",
    "dev:firefox": "webpack --env=firefox --config webpack.dev.config.js",
    "start:dev": "npm run dev:chromium"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "author": "Shuo Wu",
  "license": "GNU GPLv3",
  "dependencies": {
    "@babel/runtime": "^7.7.4",
    "@material-ui/core": "^4.9.12",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.52",
    "@yi-note/common": "^1.0.0",
    "easy-peasy": "^3.3.0",
    "i18next": "^19.0.2",
    "i18next-browser-languagedetector": "^4.0.1",
    "js-logger": "^1.6.0",
    "js-video-url-parser": "^0.4.0",
    "markerjs": "^1.8.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-highlighter": "^0.4.3",
    "react-i18next": "^11.2.6",
    "react-recipes": "^1.0.0",
    "react-router-dom": "^5.1.2",
    "prop-types": "^15.7.2",
    "styled-components": "^5.0.0-rc.2",
    "throttle-debounce": "^2.1.0",
    "uuidv4": "^6.0.0",
    "webextension-polyfill": "^0.6.0"
  }
}
